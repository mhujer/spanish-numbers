  "description": "This package provides convertor of Spanish numbers from digits to words",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "compile": "babel src/ --out-dir lib/",
    "prepublish": "npm run compile",
