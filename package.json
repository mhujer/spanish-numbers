  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhujer/spanish-numbers/issues"
  },
  "homepage": "https://github.com/mhujer/spanish-numbers#readme",
  "dependencies": {
    "babel-runtime": "6.3.19"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-core": "6.4.5",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "5.0.0",
    "mocha": "2.4.5"
  }
