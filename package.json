    "test": "mocha --compilers js:babel-core/register --reporter spec --full-trace  test/",
    "mocha": "mocha --compilers js:babel-core/register --reporter spec --full-trace --watch test/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mhujer/spanish-numbers.git"
  },
  "keywords": [
